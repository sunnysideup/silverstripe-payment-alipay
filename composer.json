{
  "name": "jq",
  "description": "Allows e-commerce payments to be made with weechat / Alipay",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "alipay"
  ],
  "license": "BSD-3-Clause",
  "require": {
    "silverstripe/framework": "^5.0",
    "silverstripe/admin": "^2.0"
  },
  "require-dev": {
    "sunnysideup/sswebpack_engine_only": "dev-master",
    "silverleague/ideannotator": "*"
  },
  "autoload": {
    "psr-4": {
      "Sunnysideup\\PaymentAliPay\\": "src/"
    }
  },
  "extra": {
    "expose": [
      "client/dist"
    ]
  },
  "bin": [
    "bin/sake-npm-build-payment-alipay.sh",
    "bin/sake-npm-watch-payment-alipay.sh"
  ],
  "authors": [
    {
      "name": "Nicolaas Thiemen Francken",
      "email": "modules@sunnysideup.co.nz",
      "role": "lead developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
